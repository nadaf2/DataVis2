{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/nadaf2/Data-Visualization-II/main/Data-Visualization-II/data/OccupationData.csv"},
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "Employment by occupation",
        "type": "ordinal",
        "title": "Employment by occupation"
      },
      "y": {
        "field": "Percentage",
        "type": "quantitative"
      },
      "color": {
        "field": "Gender",
        "type": "nominal",
        "scale": {
          "domain": ["Female", "Male"],
          "range": ["#e7ba52", "#1f77b4", "#aec7e8", "#1f77b4", "#9467bd"]
        },
        "title": "Sex"
      },
      "tooltip": [{ "field": "Employment by occupation", "type": "ordinal"} , 
                  {"field": "Percentage", "type": "quantitative"}]
    }
  }